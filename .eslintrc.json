{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-unused-vars": "off"
    },
    "globals": {
        "logger": "readonly", 
        "tracer": "readonly", 
        "db": "readonly", 
        "CustomError": "readonly", 
        "handleError" : "readonly", 
        "validate" : "readonly"
    }
}
